            <div class="col-12 col-sm-6 col-md-3 mb-4 mb-md-0">
              <div class="accordion mt-3">
                <div class="card accordion-item">
                  <h2 class="accordion-header" id="headingOne">
                    <button type="button" class="accordion-button collapsed" data-bs-toggle="collapse"
                      data-bs-target="#accordionOne" aria-expanded="true" aria-controls="accordionOne">
                      Ana Kategori
                    </button>
                  </h2>

                  <div id="accordionOne" class="accordion-collapse collapse" data-bs-parent="#accordionExample">
                    <div class="accordion-body">

                      <table class="table">
                        <thead>
                          <tr>
                            <th scope="col">İd</th>
                            <th scope="col">tr</th>
                            <th scope="col">En</th>
                            <th scope="col">fr</th>
                            <th scope="col">ar</th>
                            <th scope="col">Sil</th>
                          </tr>
                        </thead>
                        <tbody>

                          <% anakategori.forEach(v=> {%>
                            <tr>
                              <th scope="row">
                                <%= v.id %>
                              </th>
                              <td>
                                <%= v.urunAnaKategori.tr _%>
                              </td>
                              <td>
                                <%= v.urunAnaKategori.en _%>
                              </td>
                              <td>
                                <%= v.urunAnaKategori.fr _%>
                              </td>
                              <td>
                                <%= v.urunAnaKategori.ar _%>
                              </td>
                              <td>
                                <select id="<%= v.id %>" class="form-select">
                                  <option value="null">Seçiniz</option>
                                  <option value="delete">Sil</option>
                                </select>
                              </td>
                            </tr>
                            <% }); %>
                              <tr>
                                <td colspan="4" class="text-end">
                                  <ul class="list-group w-70">
                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                      <input name="anakategoritr" type="text" class="form-control me-5"
                                        placeholder="Türkçe olarak giriş yapınız" aria-describedby="floatingInputHelp">
                                      <span class="badge bg-danger rounded-pill"> <!--bg-success-->
                                        <i class="d-none tf-icons bx bx-check"></i>
                                        <i class="tf-icons bx bx-minus"></i>
                                      </span>
                                    </li>
                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                      <input name="anakategorien" type="text" class="form-control  me-5"
                                        placeholder="İngilizce olarak giriş yapınız"
                                        aria-describedby="floatingInputHelp">
                                      <span class="badge bg-danger rounded-pill"> <!--bg-success-->
                                        <i class="d-none tf-icons bx bx-check"></i>
                                        <i class="tf-icons bx bx-minus"></i>
                                      </span>
                                    </li>
                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                      <input name="anakategorifr" type="text" class="form-control me-5"
                                        placeholder="Fransızca olarak giriş yapınız"
                                        aria-describedby="floatingInputHelp">
                                      <span class="badge bg-danger rounded-pill"> <!--bg-success-->
                                        <i class="d-none tf-icons bx bx-check"></i>
                                        <i class="tf-icons bx bx-minus"></i>
                                      </span>
                                    </li>
                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                      <input name="anakategoriar" type="text" class="form-control me-5"
                                        placeholder="Arapça olarak giriş yapınız" aria-describedby="floatingInputHelp">
                                      <span class="badge bg-danger rounded-pill"> <!--bg-success-->
                                        <i class="d-none tf-icons bx bx-check"></i>
                                        <i class="tf-icons bx bx-minus"></i>
                                      </span>
                                    </li>
                                  </ul>
                                </td>
                                <td colspan="6" class="text-end">
                                  <button id="anakategori" name="kategoriekle" type="button" class="btn btn-secondary">
